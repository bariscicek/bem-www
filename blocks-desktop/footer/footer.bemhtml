block footer, !this.elem {
    default, !this.ctx._done: {
        apply(
            this._mode = '',
            this.ctx = {
                block: 'footer',
                content: [
                    {
                        elem: 'copyright',
                        content: 'Built with BEM'
                    }
                ],
                _done: true
            }
        );
    }
}